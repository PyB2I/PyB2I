   "metadata": {},
   "outputs": [],
   "source": [
    "emptyDict = {}\n",
    "print(\"Diccionario vacío: \", emptyDict)\n",
    "print(\"Tipo de dato\", type(emptyDict))"
   ]
  },
  {
   "cell_type": "code",
